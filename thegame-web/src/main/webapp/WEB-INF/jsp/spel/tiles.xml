<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN" "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<tiles-definitions>


	<!-- Definitions hun naam moeten mappen op de return value van een methode 
		op de controller -->
	<definition name="/spel/spelLijstTileDefinition" extends="globalTemplateTileDefinition">
		<put-attribute name="content" value="/WEB-INF/jsp/spel/spelLijst.jsp" />
		<!-- pagina specifieke css -->
		<put-list-attribute name="additionalCss">
			<add-attribute value="spel/spelLijst.css" />
		</put-list-attribute>
		<!-- pagina specifieke javascript files -->
		<put-list-attribute name="additionalJs">
			<add-attribute value="spel/spelLijst.js" />
		</put-list-attribute>
	</definition>

</tiles-definitions>